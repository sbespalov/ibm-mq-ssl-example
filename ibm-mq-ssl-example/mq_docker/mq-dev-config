* Create channel
DEFINE CHANNEL(QM1.SVRCONN) CHLTYPE(SVRCONN)

ALTER CHL('QM1.SVRCONN') CHLTYPE(SVRCONN) MCAUSER('sbespalov')

* Create listener
DEFINE LISTENER(LIST1) TRPTYPE(TCP) PORT(1414) CONTROL(QMGR)
START LISTENER(LIST1)

* Create queues
DEFINE QLOCAL(ESB.CF.IN)
DEFINE QLOCAL(ESB.CF.OUT)
DEFINE QLOCAL(ESB.CF.REQUEST)
DEFINE QLOCAL(ESB.CF.RESPONSE)

ALTER QMGR CHLAUTH(DISABLED)
DISPLAY QMGR SSLKEYR

ALTER CHL(QM1.SVRCONN) CHLTYPE(SVRCONN) SSLCIPH(RC4_MD5_US) SSLCAUTH(OPTIONAL)
REFRESH SECURITY TYPE(SSL)

ALTER QMGR CHLAUTH(ENABLED)
SET CHLAUTH('QM1.SVRCONN') TYPE (SSLPEERMAP) SSLPEER('CN=*') USERSRC(MAP) MCAUSER('cf99usr')
SET CHLAUTH(QM1.SVRCONN) TYPE(ADDRESSMAP) ADDRESS('*')  USERSRC(CHANNEL)
SET CHLAUTH('QM1.SVRCONN') TYPE(BLOCKUSER) USERLIST(ALLOWANY)

REFRESH SECURITY TYPE(SSL)
REFRESH SECURITY TYPE(AUTHSERV)

END